#kprfmap .window h1.title { padding-top: 6px; text-transform: uppercase; font-weight: 900; }

#kprfmap .regionselector { margin-top: 16px; background: url("../images/bg_menu.png") repeat-x rgb(255, 255, 255); border-width: 1px; border-style: solid; border-color: rgb(209, 209, 209) rgb(184, 187, 188) rgb(184, 187, 188); height: 300px; }

#kprfmap .regionselector ul { list-style: none; }

#kprfmap .regionselector li { margin: 0px; display: block; float: left; border-width: 1px; border-style: solid; border-color: white rgb(147, 150, 151) rgb(184, 187, 188) rgb(239, 239, 239); color: black; font: 12px / 15px arial; text-shadow: white 1px 1px; padding: 6px 8px 7px; text-align: center; cursor: pointer; }

#kprfmap .regionselector li.last { border-right: none; }

#kprfmap .regionselector li.show { background-color: white; border-bottom: 1px solid white; }

#kprfmap .regionselector .info { clear: both; display: none; padding-left: 24px; background-color: white; }

#kprfmap .regionselector .info ul { display: block; float: left; width: 200px; margin: 32px 0px 0px 16px; }

#kprfmap .regionselector .info li { display: block; float: none; border: none; text-align: left; }

#kprfmap .regionselector a { color: rgb(205, 7, 8); font: 13px / 14px arial; }

