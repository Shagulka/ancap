.TPL .colleft { width: 200px; padding: 0px; }

.TPL .colcontent { padding: 0px 0px 0px 8px; }

.TPL .colright { width: 200px; padding-left: 16px; }

.TPL .colone { padding: 16px 24px 0px; }

.TPL h1 { margin-bottom: 26px; }

.TPL .no_red { color: rgb(102, 102, 102); }

.TPL a { color: rgb(204, 0, 0); }

.TPL .breadcrumb { margin: 0px 0px 16px; font-size: 11px; font-family: Arial; font-weight: normal; }

.TPL .breadcrumb a { color: black; padding: 0px 3px; }

